package edu.fjnu.xtw.domain;

/**
 * @author lzx
 * 
 */
public class XtwKeyWord {
	private Integer keywordId;
	private String keywordOne;
	private String keywordTwo;
	private String keywordThree;
	private String keywordFour;
	private String keywordFive;
	private String keywordSix;
	private String keywordSeven;
	private String keywordEight;
	private String keywordNine;
	private String keywordTen;

	public XtwKeyWord() {
		super();
	}

	public XtwKeyWord(Integer keywordId, String keywordOne, String keywordTwo,
			String keywordThree, String keywordFour, String keywordFive,
			String keywordSix, String keywordSeven, String keywordEight,
			String keywordNine, String keywordTen) {
		super();
		this.keywordId = keywordId;
		this.keywordOne = keywordOne;
		this.keywordTwo = keywordTwo;
		this.keywordThree = keywordThree;
		this.keywordFour = keywordFour;
		this.keywordFive = keywordFive;
		this.keywordSix = keywordSix;
		this.keywordSeven = keywordSeven;
		this.keywordEight = keywordEight;
		this.keywordNine = keywordNine;
		this.keywordTen = keywordTen;
	}

	public Integer getKeywordId() {
		return keywordId;
	}

	public void setKeywordId(Integer keywordId) {
		this.keywordId = keywordId;
	}

	public String getKeywordOne() {
		return keywordOne;
	}

	public void setKeywordOne(String keywordOne) {
		this.keywordOne = keywordOne;
	}

	public String getKeywordTwo() {
		return keywordTwo;
	}

	public void setKeywordTwo(String keywordTwo) {
		this.keywordTwo = keywordTwo;
	}

	public String getKeywordThree() {
		return keywordThree;
	}

	public void setKeywordThree(String keywordThree) {
		this.keywordThree = keywordThree;
	}

	public String getKeywordFour() {
		return keywordFour;
	}

	public void setKeywordFour(String keywordFour) {
		this.keywordFour = keywordFour;
	}

	public String getKeywordFive() {
		return keywordFive;
	}

	public void setKeywordFive(String keywordFive) {
		this.keywordFive = keywordFive;
	}

	public String getKeywordSix() {
		return keywordSix;
	}

	public void setKeywordSix(String keywordSix) {
		this.keywordSix = keywordSix;
	}

	public String getKeywordSeven() {
		return keywordSeven;
	}

	public void setKeywordSeven(String keywordSeven) {
		this.keywordSeven = keywordSeven;
	}

	public String getKeywordEight() {
		return keywordEight;
	}

	public void setKeywordEight(String keywordEight) {
		this.keywordEight = keywordEight;
	}

	public String getKeywordNine() {
		return keywordNine;
	}

	public void setKeywordNine(String keywordNine) {
		this.keywordNine = keywordNine;
	}

	public String getKeywordTen() {
		return keywordTen;
	}

	public void setKeywordTen(String keywordTen) {
		this.keywordTen = keywordTen;
	}

	@Override
	public String toString() {
		return "XtwKeyWord [keywordId=" + keywordId + ", keywordOne="
				+ keywordOne + ", keywordTwo=" + keywordTwo + ", keywordThree="
				+ keywordThree + ", keywordFour=" + keywordFour
				+ ", keywordFive=" + keywordFive + ", keywordSix=" + keywordSix
				+ ", keywordSeven=" + keywordSeven + ", keywordEight="
				+ keywordEight + ", keywordNine=" + keywordNine
				+ ", keywordTen=" + keywordTen + "]";
	}
	
	/**
	 * 通过id，来决定关键词插入到那个属性中
	 * @param id
	 * @param keyWord
	 */
	public void setKeyWordById(Integer id, String keyWord){
		switch (id) {
		case 1:
			this.setKeywordOne(keyWord);
			break;
		case 2:
			this.setKeywordTwo(keyWord);
			break;
		case 3:
			this.setKeywordThree(keyWord);
			break;
		case 4:
			this.setKeywordFour(keyWord);
			break;
		case 5:
			this.setKeywordFive(keyWord);
			break;
		case 6:
			this.setKeywordSix(keyWord);
			break;
		case 7:
			this.setKeywordSeven(keyWord);
			break;
		case 8:
			this.setKeywordEight(keyWord);
			break;
		case 9:
			this.setKeywordNine(keyWord);
			break;
		case 10:
			this.setKeywordTen(keyWord);
			break;
		default:
			break;
		}
	}

}
